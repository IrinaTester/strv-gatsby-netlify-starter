{
  "name": "strv-gatsby-netlify-starter",
  "description": "STRV Starter using Gatsby & Netlify CMS",
  "version": "0.0.1",
  "author": "Peter Varholak",
  "dependencies": {
    "gatsby": "^2.13.31",
    "gatsby-image": "^2.0.23",
    "gatsby-plugin-manifest": "^2.2.23",
    "gatsby-plugin-netlify": "^2.0.6",
    "gatsby-plugin-netlify-cms": "^4.1.6",
    "gatsby-plugin-offline": "^3.0.16",
    "gatsby-plugin-page-creator": "^2.1.27",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-remark-copy-linked-files": "^2.0.7",
    "gatsby-remark-images": "^3.1.6",
    "gatsby-remark-relative-images": "^0.2.1",
    "gatsby-source-filesystem": "^2.0.26",
    "gatsby-transformer-remark": "^2.6.9",
    "gatsby-transformer-sharp": "^2.1.9",
    "netlify-cms-app": "^2.9.6",
    "parcel-bundler": "^1.9.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "react",
    "gatsby",
    "netlify",
    "netlify-cms",
    "typescript",
    "styled-components",
    "starter"
  ],
  "license": "MIT",
  "scripts": {
    "apollo:download": "apollo service:download -c apollo.config.download.js",
    "apollo:gen": "apollo client:codegen -c apollo.config.js --namespace GTypes --target typescript --outputFlat --tsFileExtension d.ts src/@generatedTypes",
    "build": "npm run clean && gatsby build",
    "clean": "gatsby clean",
    "dev": "npm run clean && gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "apollo": "^2.20.0",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-typescript": "^2.1.15",
    "prettier": "^1.15.3"
  }
}
